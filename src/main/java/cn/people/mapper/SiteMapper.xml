<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.people.mapper.SiteMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.people.entity.Site">
    <result column="id" property="id" />
        <result column="sitename" property="sitename" />
        <result column="description" property="description" />
        <result column="domain" property="domain" />
        <result column="createtime" property="createtime" />
        <result column="createrid" property="createrid" />
        <result column="orgid" property="orgid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        sitename, description, domain, createtime, createrid, orgid
    </sql>

</mapper>
